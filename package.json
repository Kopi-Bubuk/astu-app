  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "android": "react-native run-android --variant=stagingdebug",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:dev": "react-native run-android --variant=developmentdebug start --port=8081",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "android:clean": "cd android && ./gradlew clean",
    "android:assembly": "cd android && ./gradlew assembleRelease",
    "android:build-staging": "cd android && APP_ENV=.env.staging ./gradlew assembleStagingRelease",
    "android:build-prod": "cd android && ./gradlew assembleProductionRelease",
    "ios": "react-native run-ios",
    "ios:podInstall": "cd ios && pod install",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\"",
    "clean": "npm run clean",
    "build:ios": "react-native run-ios --configuration=release",
    "build:android": "npm run android:clean && npm run android:assembly && react-native run-android --variant=release",
    "postinstall": "rn-nodeify --install assert,buffer,process,crypto,stream,vm --hack"
  },
